#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    float liters, distance, liters_per_100km, miles_per_gallon;


    if (liters <= 0 || distance <= 0) {
        printf("Invalid Input\n");
        return 0;
    }

    liters_per_100km = (liters / distance) * 100;
    miles_per_gallon = (distance * 0.6214) / (liters * 0.2642);

    printf("Liters/100KM");
            printf(" %.2f\n", liters_per_100km);
    printf("Miles/gallons");
           printf("%.2f\n", miles_per_gallon);

    return 0;
}


