#include <stdio.h>

int main() {
    int n, allEven = 1, allOdd = 1;



    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);

        if (arr[i] % 2 == 0) {
            allOdd = 0;
        } else {
            allEven = 0;
        }
    }

    if (allEven) {
        printf("Even\n");
    } else if (allOdd) {
        printf("Odd\n");
    } else {
        printf("Mixed\n");
    }

    return 0;
}

