#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 char name[100];
 int arears;
 float cgpa;
    scanf("%s %d %f", name,&arears,&cgpa);
    printf("Name of the Student:%s\n",name);
    if((arears==1&&cgpa>=70)||(arears<=2&&cgpa>=75)){
        printf("%s is Eligible for Placement",name);
        }
    else{
        printf("%s is Eligible for Placement",name);
        }
    return 0;
    }
